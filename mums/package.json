{
  "name": "react-ecosystem-workshop",
  "version": "1.0.0",
  "description": "A practical workshop for learning React and its ecosystem",
  "main": "index.js",
  "scripts": {
    "test": "mocha --compilers js:babel-register --recursive ./**/*.spec.js",
    "test:watch": "npm test -- --watch",
    "build:webpack": "webpack --config webpack.config.js -p",
    "build": "npm run clean && npm run build:webpack",
    "clean": "rimraf dist/*.bundle.*",
    "start": "webpack-dev-server --hot --inline --config webpack.config.js"
  },
  "author": "Rodrigo Arg√ºello Flores",
  "license": "ISC",
  "devDependencies": {
    "eslint": "^3.14.1",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-babel": "^4.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "expect": "^1.20.2",
    "expect.js": "^0.3.1",
    "mocha": "^3.2.0",
    "react-hot-loader": "^1.3.1",
    "webpack-dev-server": "^2.2.1"
  },
  "dependencies": {
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-object-rest-spread": "^6.22.0",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "css-loader": "^0.26.1",
    "file-loader": "^0.10.0",
    "lodash.groupby": "^4.6.0",
    "lodash.orderby": "^4.6.0",
    "lodash.zip": "^4.2.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "redux": "^3.6.0",
